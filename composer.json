{
    "name": "lumensolucoes/filament-asaas",
    "keywords": [
        "lumensolucoes",
        "laravel",
        "filament",
        "filament-asaas",
        "asaas"
    ],
    "description": "Filament plugin for integrating Asaas (payments) with multi-tenancy support",
    "homepage": "https://github.com/lumensolucoes/filament-asaas",
    "support": {
        "issues": "https://github.com/lumensolucoes/filament-asaas/issues",
        "source": "https://github.com/lumensolucoes/filament-asaas"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lumen Soluções",
            "email": "lumensolucoes@gmail.com",
            "role": "Developer"
        }
    ],
    "type": "library",
    "version": "0.0.1",
    "autoload": {
        "psr-4": {
            "lumensolucoes\\FilamentAsaas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lumensolucoes\\FilamentAsaas\\tests": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0",
        "filament/filament": "^3.0 || ^2.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "laradumps/laradumps": "^3.1",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9|^8.6",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.1|^3.4.7",
        "pestphp/pest-plugin-arch": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "lumensolucoes\\FilamentAsaas\\AsaasServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}