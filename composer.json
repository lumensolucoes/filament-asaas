{
    "name": "lumensolucoes/filament-asaas",
    "description": "Filament v4 plugin for integrating Asaas (payments) with multi-tenancy support",
    "keywords": [
        "lumensolucoes",
        "laravel",
        "filament",
        "filament-asaas",
        "asaas",
        "payments",
        "multi-tenant"
    ],
    "homepage": "https://github.com/lumensolucoes/filament-asaas",
    "license": "MIT",
    "authors": [
        {
            "name": "Lumen Soluções",
            "email": "lumensolucoes@gmail.com",
            "role": "Developer"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "^7.0",
        "filament/filament": "^4.0",
        "illuminate/support": "^11.0",
        "illuminate/contracts": "^11.0"
    },
    "require-dev": {
        "laradumps/laradumps": "^3.1",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.6",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "lumensolucoes\\filamentasaas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lumensolucoes\\filamentasaas\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        },
        "laravel": {
            "providers": [
                "lumensolucoes\\filamentasaas\\AsaasServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}